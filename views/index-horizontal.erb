<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8" />
  <link rel="stylesheet" href="styles/main.css">
  <title>NeonSwipe - Horizontal</title>

    <script type="text/javascript" src="vendor/jquery-1.7.1.min.js"></script>

  <script src="motionDetector.js" type="text/javascript"></script>
  <script src="app.js" type="text/javascript"></script>
  <link href="vendor/bootstrap/css/bootstrap.min.css" rel="stylesheet" media="screen">
  <script src="vendor/bootstrap/js/bootstrap.min.js"></script>
  <script type="text/javascript">
      if(document.addEventListener){
          document.addEventListener('DOMContentLoaded', function(){
              new App(false);
          });
      } else {
          window.onload = function(){
              new App(false);
          };
      }
  </script>
</head>
<body>
  <video autoplay="autoplay" id="canvasVideo" style="display: none;"></video>
  <canvas id="canvas" class="canvas mini" width="128px" height="96px"></canvas>
  <div class="offset4 span8">
    <div id="myCarousel" class="carousel slide">
      <ol class="carousel-indicators">
        <li data-target="#myCarousel" data-slide-to="0" class="active"></li>
        <li data-target="#myCarousel" data-slide-to="1"></li>
        <li data-target="#myCarousel" data-slide-to="2"></li>
      </ol>
      <!-- Carousel items -->
      <div class="carousel-inner">
        <div class="active item"><img src="images/img1.jpg" class="image"></div>
        <div class="item"><img src="images/img2.jpg" class="image"></div>
        <div class="item"><img src="images/img3.jpg" class="image"></div>
        <div class="item"><img src="images/img4.jpg" class="image"></div>
      </div>
      <!-- Carousel nav -->
      <a class="carousel-control left" href="#myCarousel" data-slide="prev">&lsaquo;</a>
      <a class="carousel-control right" href="#myCarousel" data-slide="next">&rsaquo;</a>
    </div>
  </div>
</body>
</html>
